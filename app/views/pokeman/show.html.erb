<table class="table">
  <thead>
    <tr>
      <th>Entry</th>
      <th>Name</th>
      <th>Type 1</th>
      <th>Type 2</th>
      <th>Total Stats</th>
      <th>HP</th>
      <th>Attack</th>
      <th>Defense</th>
      <th>Sp. Atk</th>
      <th>Sp. Def</th>
      <th>Speed</th>
      <th>Generation</th>
    </tr>
  </thead>
  <tbody>
      <tr>
        <th><%= @pokemon.entryNumber.round %></th>
